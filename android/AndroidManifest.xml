<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.hfoss.posit">
	<application android:icon="@drawable/icon" android:debuggable="true">
	<uses-library android:name="com.google.android.maps" />
		<activity android:name=".PositMain" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<!--<activity android:name=".Mapper"></activity>
		-->
		<activity android:name=".NewFind">
			<intent-filter>
			<action android:name="org.hfoss.posit.find.refreshInstance"/>
			<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
		<receiver android:name=".MyIntentReceiver">
			<intent-filter>
			<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
		</receiver>
		</activity>

		<activity android:name=".Preferences"></activity>
		<activity android:name=".PictureViewer"></activity>
		<activity android:name=".ListFinds"></activity>
		<activity android:name=".MapperNew" android:exported="true" android:excludeFromRecents="true">
		<activity android:name=".ListFindOnServer"></activity>
		</activity>
		<activity android:name=".SendFiles"></activity>
		<activity android:name=".CameraPreview2"></activity>
		<activity android:name=".CameraPreview3"></activity>
		<activity android:name=".GetDataFromWeb"></activity>
		<activity android:name=".NewSighting"></activity><!--
		<activity android:name=".ViewSightings"></activity>
	<activity android:name="ListFindOnServer" android:enabled="true"></activity>
	-->
<activity android:enabled="true" android:name=".MapTrackingVersion" android:label="Explore"></activity>
</application>
	
	


<uses-permission android:name="android.permission.CAMERA"></uses-permission>
<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>


<uses-permission android:name="android.permission.BROADCAST_STICKY"></uses-permission>
<uses-permission android:name="android.permission.INTERNET"></uses-permission>




<uses-permission android:name="android.permission.CONTROL_LOCATION_UPDATES"></uses-permission>
<uses-permission android:name="android.permission.READ_INPUT_STATE"></uses-permission>




</manifest>